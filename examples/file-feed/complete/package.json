{
  "name": "file-feed",
  "version": "0.0.0",
  "devDependencies": {
    "buffer": "^5.0.2",
    "react-scripts": "0.8.5",
    "uglify-js": "github:mishoo/UglifyJS2#harmony"
  },
  "dependencies": {
    "browserify-zlib-next": "^1.0.0",
    "ipfs": "^0.21.0",
    "ipfs-daemon": "^0.3.0-beta.18",
    "mime": "^1.3.4",
    "orbit-db": "^0.16.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.9.0",
    "react-router": "^3.0.1"
  },
  "scripts": {
    "check-bundle": "test -f ../../../dist/index.js && npm run copy-bundle || echo \"js-ipfs dist file not found, go up two dirs and run 'npm run build' first\"",
    "copy-bundle": "cp ../../../dist/index.js ./public/js/ipfs.js",
    "start": "npm run check-bundle && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
