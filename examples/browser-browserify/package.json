{
  "name": "example-browser-browserify",
  "description": "Bundle js-ipfs with Browserify",
  "version": "1.1.1",
  "main": "index.js",
  "private": true,
  "scripts": {
    "bundle": "browserify src/index.js > public/bundle.js",
