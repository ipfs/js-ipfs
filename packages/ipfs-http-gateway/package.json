    "hapi-pino": "^8.3.0",
    "ipfs-core-utils": "^0.7.1",
    "ipfs-http-response": "^0.6.0",
    "is-ipfs": "^2.0.0",
    "it-last": "^1.0.4",
    "it-to-stream": "^0.1.2",
    "joi": "^17.2.1",
    "multibase": "^3.0.0",
    "uint8arrays": "^2.0.5",
    "uri-to-multiaddr": "^4.0.0"
  },
  "devDependencies": {
    "aegir": "ipfs/aegir#feat/remove-webpack-karma",
