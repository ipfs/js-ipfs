{
  "name": "ipfs-cli",
  "version": "0.12.3",
  "description": "JavaScript implementation of the IPFS specification",
  "keywords": [
    "IPFS"
  ],
  "homepage": "https://github.com/ipfs/js-ipfs/tree/master/packages/ipfs-cli#readme",
  "bugs": "https://github.com/ipfs/js-ipfs/issues",
  "license": "(Apache-2.0 OR MIT)",
  "type": "module",
  "main": "src/index.js",
  "types": "types/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "types/*",
        "types/src/*"
      ],
      "types/*": [
        "types/*",
        "types/src/*"
      ]
    }
  },
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./utils": {
      "import": "./src/utils.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/js-ipfs.git"
  },
  "scripts": {
    "lint": "aegir lint",
    "test": "npm run test:node",
    "test:node": "aegir test -t node --cov",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check -i ipfs-core-types",
    "build": "aegir build --no-bundle"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^7.0.0",
    "@ipld/dag-json": "^8.0.1",
    "@ipld/dag-pb": "^2.1.3",
    "@libp2p/crypto": "^0.22.9",
    "@multiformats/mafmt": "^11.0.2",
    "@multiformats/multiaddr": "^10.0.0",
    "@multiformats/multiaddr-to-uri": "^9.0.1",
    "byteman": "^1.3.5",
    "debug": "^4.1.1",
    "err-code": "^3.0.1",
    "execa": "^6.1.0",
    "get-folder-size": "^3.1.0",
    "ipfs-core": "^0.14.2",
    "ipfs-core-types": "^0.10.2",
    "ipfs-core-utils": "^0.14.2",
    "ipfs-daemon": "^0.12.2",
    "ipfs-http-client": "^56.0.2",
    "ipfs-repo": "^14.0.1",
    "ipfs-utils": "^9.0.6",
    "it-all": "^1.0.4",
    "it-concat": "^2.0.0",
    "it-first": "^1.0.4",
    "it-glob": "^1.0.0",
    "it-map": "^1.0.5",
    "it-merge": "^1.0.3",
    "it-pipe": "^2.0.3",
    "it-split": "^1.0.0",
    "it-tar": "^5.0.0",
    "jsondiffpatch": "^0.4.1",
    "multiformats": "^9.5.1",
    "parse-duration": "^1.0.0",
    "pretty-bytes": "^6.0.0",
    "progress": "^2.0.3",
    "stream-to-it": "^0.2.2",
    "uint8arrays": "^3.0.0",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@types/ncp": "^2.0.5",
    "@types/progress": "^2.0.3",
    "@types/yargs": "^17.0.10",
    "aegir": "^37.0.11",
    "it-to-buffer": "^2.0.0",
    "nanoid": "^3.1.23",
    "ncp": "^2.0.0",
    "pako": "^2.0.4",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "string-argv": "^0.3.1",
    "temp-write": "^5.0.0"
  }
}
