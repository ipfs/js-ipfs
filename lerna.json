{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "useWorkspaces": true,
  "version": "0.0.1-87d6a2c5d.0",
  "command": {
    "publish": {
      "message": "chore: publish",
      "createRelease": "github",
      "conventionalCommits": true,
      "verifyAccess": false
    },
    "version": {
      "allowBranch": ["master", "release/*"],
      "forcePublish": true,
      "ignore-changes": [".github/**", "docs/**"]
    }
  }
}
